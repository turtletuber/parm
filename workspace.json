              "**/node_modules/**",
              "!apps/react/**"
            ]
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": ".storybook/react"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build": {
          "builder": "@nrwl/storybook:build",
